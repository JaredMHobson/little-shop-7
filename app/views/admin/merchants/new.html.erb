<div class="container text-center">
  <%= render 'shared/admin_navbar' %>
  <div class="row">
    <h1 class="headings">New Merchant</h1>
  </div>

  <div class="row">
    <div class="col">
      <%= form_with model: @merchant, data: {turbo: false} do |f| %>
        <%= f.label :name, 'Name:' %>
        <%= f.text_field :name %>
        <%= f.submit %>
      <% end %>
    </div>
    <div class="col">
    </div>
  </div>
</div>
