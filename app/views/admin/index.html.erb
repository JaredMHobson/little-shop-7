<h1>Admin Dashboard</h1>

<%= link_to 'All Merchants', admin_merchants_path %><br>
<%= link_to 'All Invoices', admin_invoices_path %><br>

<div id= "top five customers">
  <h2>Top 5 Customers</h2>
  <ul>
  <% @customers.goated_five_customers.each do |customer| %>
    <li><%= "Customer name: #{customer.first_name} #{customer.last_name} 
    - #{customer.transaction_count} successful transactions" %><br>
    </li>
  <% end %>
  </ul>
</div>